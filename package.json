    "lint-staged": "^13.0.3",
    "mockjs": "^1.0.1-beta3",
    "peggy": "^2.0.1",
    "prettier": "^2.3.2",
    "puppeteer-core": "^8.0.0",
    "stylelint": "^13.0.0",
    "typescript": "^4.2.2"
  },
  "resolutions": {
