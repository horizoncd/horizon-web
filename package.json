    "pikaday": "^1.8.2",
    "prop-types": "^15.8.1",
    "qiankun": "^2.8.1",
    "query-string": "^7.1.1",
    "rc-field-form": "^1.27.1",
    "react": "^17.0.0",
    "react-bootstrap": "^2.4.0",
    "react-codemirror2": "^7.2.1",
    "react-dev-inspector": "^1.1.1",
